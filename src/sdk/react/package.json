{
  "name": "@traf3li/auth-react",
  "version": "1.0.0",
  "description": "React SDK for Traf3li Auth",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --external react --external @traf3li/auth-core",
    "build:dts": "tsup src/index.ts --format cjs,esm --dts --external react --external @traf3li/auth-core",
    "dev": "tsup src/index.ts --format cjs,esm --external react --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@traf3li/auth-core": "file:../core"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "traf3li",
    "auth",
    "react",
    "hooks"
  ],
  "license": "MIT"
}
