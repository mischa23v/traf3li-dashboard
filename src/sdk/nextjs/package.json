{
  "name": "@traf3li/auth-nextjs",
  "version": "1.0.0",
  "description": "Next.js SDK for Traf3li Auth",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./middleware": {
      "import": "./dist/middleware/index.mjs",
      "require": "./dist/middleware/index.js",
      "types": "./dist/middleware/index.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    },
    "./api": {
      "import": "./dist/api/index.mjs",
      "require": "./dist/api/index.js",
      "types": "./dist/api/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/middleware/index.ts src/server/index.ts src/api/index.ts --format cjs,esm --dts --external react --external next",
    "dev": "tsup src/index.ts src/middleware/index.ts src/server/index.ts src/api/index.ts --format cjs,esm --dts --external react --external next --watch",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0",
    "@traf3li/auth-core": "^1.0.0",
    "@traf3li/auth-react": "^1.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "next": "^14.0.0",
    "react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "traf3li",
    "auth",
    "nextjs",
    "middleware"
  ],
  "license": "MIT"
}
